{
  "name": "dashboard-liveagent",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --host 0.0.0.0 --port 4201",
    "build": "ng build --output-hashing=all",
    "watch": "ng build --watch --configuration development",
    "install-capacitor": "npm i @capacitor/cli @capacitor/android @capacitor/angular @capacitor/core @capacitor/storage",
    "init-capacitor": "npx cap init",
    "mobile-build": "npm run build && rmdir /s .\\android && npx cap add android && npx cap run android && npx cap copy android && npx cap sync android && npx cap open android",
    "test": "ng test"
  },
  "private": true,
